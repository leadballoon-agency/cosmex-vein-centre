{
  "version": 2,
  "builds": [
    {
      "src": "vein_ai_backend.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vein_ai_backend.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "CLAUDE_API_KEY": "@claude-api-key",
    "OPENAI_API_KEY": "@openai-api-key"
  }
}